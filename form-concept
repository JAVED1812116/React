import React from 'react';
import {useState} from 'react';
import './App.css';
// import User from './User';
// import User from './User';

function App() {
  function getFromData(e){
    console.warn(name,interest,tnc)
    e.preventDefault()
  }
  const [name,setName]=useState("");
  const [interest,setInterest]=useState("");
  const [tnc,setTnc]=useState(false);

  return (
    <div className="App">
      <form onSubmit={getFromData}>
        <input type="text" placeholder="enter your name" onChange={(e)=>setName(e.target.value)}></input><br /><br />
        <select onChange={(e)=>setInterest(e.target.value)}>
          <option>select value</option>
          <option>javed</option>
          <option>samejo</option>
        </select><br /><br />
        <input type="checkbox" onChange={(e)=>setTnc(e.target.checked)}></input><span>Accept Term And Condition</span><br /><br />
        <button type="submit">submit</button>
        <button type="reset">Reset</button>
      </form>
    </div>
  );
}

export default App;
